@CHARSET "UTF-8";

body,div,ul,li,h1,table,tr,td {
	margin: 0px;
	padding: 0px;
}

.title {
	font-size: 18px;
	color: #1A3347;
	background-color: #95BFE0;
	font-weight: normal;
	font-size: 14px;
	line-height: 30px;
	height: 30px;
}

.btn {
	border-radius: 10%;
	background-color: #D3E8F5;;
}

.query {
	position: absolute;
	top: 45px;
	left: 100px;
	font-size: 14px;
}

.modify {
	position: absolute;
	top: 80px;
	left: 95px;
	font-size: 14px;
}

.accountInfo {
	width: 600px;
	margin: 70px auto;
	margin-top: 85px;
	border-collapse: collapse;
}

.accountInfo tr {
	height: 30px;
	line-height: 30px;
	text-align: center;
}

.accountInfo tr td {
	border: 4px solid #E3E3E3;
}

.titleRow {
	background-color: #FFEED0;
	font-size: 16px;
	font-weight: bold;
}